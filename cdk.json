{
  "app": "npx ts-node bin/gen-aws.ts",
  "plugin": ["cdk-assume-role-credential-plugin"],
  "context": {
    "prj": "gen-aws",
    "@aws-cdk/core:enableStackNameDuplicates": "true",
    "aws-cdk:enableDiffNoFail": "true",
    "@aws-cdk/core:stackRelativeExports": "true",
    "assume-role-credentials:writeIamRoleName": "provision-assume-role",
    "assume-role-credentials:readIamRoleName": "provision-assume-role",
    "hostzone": "genxarch.com",
    "dev": {
      "cibr": "10.3.0.0/16",
      "maxAzs": 2
    },
    "pre": {
      "cibr": "10.2.0.0/16",
      "maxAzs": 2
    },
    "prd": {
      "cibr": "10.1.0.0/16",
      "maxAzs": 3
    }
  }
}
